{
    "name": "callmez/yii2-storage",
    "type": "yii2-extension",
    "description": "The attachment storage for the Yii framework. Support (local, qiniu) file storage,",
    "keywords": ["yii2", "attachment", "stroage", "qiniu"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/callmez/yii2-storage/issues",
        "wiki": "https://github.com/callmez/yii2-storage/wiki",
        "source": "https://github.com/callmez/yii2-storage"
    },
    "authors": [
        {
            "name": "CallMeZ",
            "email": "callme-z@qq.com"
        }
    ], 
    "require": {
        "yiisoft/yii2": "*",
        "qiniu/php-sdk": "*",
        "bower-asset/jquery.fileapi": "*"
    },
    "autoload": {
        "psr-4": {
            "yii\\storage\\": ""
        }
    }
}